{
    "name": "arokettu/torrent-file-cli",
    "type": "project",
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\Torrent\\CLI\\": "src"
        }
    },
    "bin": [
        "bin/torrent-file"
    ],
    "require": {
        "php": "^8.1",
        "ext-intl": "*",
        "composer-runtime-api": "^2.2",
        "arokettu/bencode": "^4.0",
        "arokettu/kilo-mega": "^1.0",
        "arokettu/torrent-file": "dev-master",
        "symfony/console": "^6.2",
        "symfony/var-dumper": "^6.2"
    },
    "require-dev": {
        "sandfox.dev/code-standard": "^1.2022.06.29",
        "squizlabs/php_codesniffer": "*"
    }
}
